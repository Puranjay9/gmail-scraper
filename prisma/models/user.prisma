model User {
  id          String   @id @default(cuid())
  email       String   @unique
  name        String?
  picture     String?
  googleId    String?  @unique
  provider    String   @default("google")
  isVerified  Boolean  @default(false)
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt

  @@map("users")
}